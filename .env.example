# Application Secrets
SECRET_KEY=change-me
JWT_SECRET_KEY=change-me-too
SESSION_SECRET=change-me-session

# Database Configuration
DATABASE_URL=sqlite:///quantum_nexus.db
SQLALCHEMY_DATABASE_URI=sqlite:///quantum_nexus.db
DATABASE_PATH=quantum_nexus.db

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_TTL=300

# Honeypot Settings
HONEYPOT_PUBLIC_DOMAIN=honeypot.example.com
HONEYPOT_LISTEN_PORT=8080
HONEYPOT_ADMIN_PORT=8443
HONEYPOT_ALLOWED_ORIGINS=https://console.example.com
HONEYPOT_SIGNAL_SAMPLE_RATE=0.75

# Alerting Configuration
ALERT_EMAIL_ENABLED=false
ALERT_EMAIL_SENDER=alerts@example.com
ALERT_EMAIL_RECIPIENT=security@example.com
ALERT_WEBHOOK_URL=https://hooks.example.com/security
ALERT_SLACK_WEBHOOK=https://hooks.slack.com/services/xxx/yyy/zzz
ALERT_THRESHOLD_SCORE=0.85

# Authentication / API Keys
ADMIN_DEFAULT_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD
API_KEYS_DASHBOARD=dashboard-key-placeholder
API_KEYS_AUTOMATION=automation-key-placeholder

# External Integrations
THREAT_INTEL_API_KEY=
GEOLOCATION_API_KEY=
MACHINE_LEARNING_MODEL_PATH=models/fingerprint_model.pkl

# Socket Settings
SOCKET_IO_MESSAGE_QUEUE=redis://localhost:6379/1
SOCKET_IO_NAMESPACE=/events

# Scheduler
SCHEDULER_ENABLED=true
SCHEDULER_HEARTBEAT_INTERVAL=60

# SSL / TLS
SSL_CERT_PATH=/etc/ssl/certs/honeypot.crt
SSL_KEY_PATH=/etc/ssl/private/honeypot.key
SSL_FORCE_HTTPS=true

# Logging
LOG_LEVEL=INFO
LOG_RETENTION_DAYS=14
LOG_EXPORT_DIRECTORY=exports

# Analytics / Machine Learning
ML_BACKEND=scikit-learn
TENSORFLOW_ENABLE_GPU=false
REDIS_FEATURE_FLAG_BUCKET=honeypot:feature-flags

# Miscellaneous
ENABLE_RATE_LIMITS=true
ENABLE_CORS=true
CORS_ALLOWED_ORIGINS=*
